<script>
const shopifyUrl = 'https://beckon-court.myshopify.com/admin/api/2023-01/customers.json';
const accessToken = 'Admin API access token';
const newCustomer = {
  customer: {
    first_name: 'Johnder',
    last_name: 'Doe',
    email: 'john.doe@example.com',
    tags: 'VIP, Newsletter',
    note: 'Frequent buyer',
    verified_email: true
  }
};

fetch(shopifyUrl, {
  method: 'POST',
  headers: {
    'X-Shopify-Access-Token': accessToken,
    'Content-Type': 'application/json',
  },
  body: JSON.stringify(newCustomer),
})
  .then(response => response.json())
  .then(data => {
    if (data.errors) {
      console.error('Error creating new customer:', data.errors);
    } else {
      console.log('New customer created:', data.customer);
    }
  })
  .catch(error => {
    console.error('Network error:', error);
  });
</script>